{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.{css,js,ico,svg,png,jpg,jpeg,gif,woff,woff2,ttf,eot}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/CSS/(.*)",
      "dest": "/CSS/$1"
    },
    {
      "src": "^/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "^/components/(.*)",
      "dest": "/components/$1"
    },
    {
      "src": "^/pages/(.*)",
      "dest": "/pages/$1"
    },
    {
      "src": "^/([^/]+)\\.html$",
      "dest": "/$1.html"
    },
    {
      "src": "^/([^/]+)/?$",
      "dest": "/$1.html"
    },
    {
      "src": "^/",
      "dest": "/index.html"
    },
    {
      "src": "^/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
